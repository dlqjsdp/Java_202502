package ch01.ex06;

public class Account {
	public static int MIN_BALANCE = 0;
	public static int MAX_BALANCE = 1000000;
	private int balance;
	
	int getBalance() {
		return balance;
	}
	
	void setBalance(int balance) {
		if(balance<=Account.MAX_BALANCE||balance>Account.MAX_BALANCE)
		this.balance = balance;		
	}
	

}
