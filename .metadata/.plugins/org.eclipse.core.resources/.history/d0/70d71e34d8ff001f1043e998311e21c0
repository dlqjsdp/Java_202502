package ex05;

public class LgTV extends TV{

   
   public void power() {
      if(power = false) {
    	  System.out.println("LGTV의 전원이 꺼집니다.");
      }else{
         System.out.println("LGTV의 전원이 켜졌습니다.");
      }
   }
   

   public void volume() {
      if(volume>=0) {
         System.out.println("LGTV의 사운드를 올립니다.");
      }else{
         System.out.println("LGTV의 사운드를 내립니다.");
      }   
   }
}
