package ex05;

public class GuGuDanExpr extends Multiplication {
	
	public GuGuDanExpr() {
		
	}
	
	public GuGuDanExpr(int dan){
		super();
	}
	
	public GuGuDanExpr(int dan, int number){
		super(dan, number);
	}
	
	public void printAll() {
		System.out.print("\t"+ dan + "*" + n + "=" + dan*n);
		
	}

}
