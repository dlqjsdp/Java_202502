package ch01.ex09;

public class A {
	String name;
	
	A(String name){
		this.name = name;
	}
}

class B extends A{
	String address;
	
	B(String name, String address){
		super(name);
		this.address = address;
	}
}

class C extends B{
	int age;
	
	C(String name, String address, int age){
		super(name, address);
		this.age = age;
	}
}