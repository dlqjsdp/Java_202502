package ch01.ex06;

public class Account {
	static final int MIN_BALANCE = 0;
	static final int MAX_BALANCE = 1000000;
	private int balance;
	
	int getBalance() {
		return balance;
	}
	
	void setBalance(int balance) {
		if(balance<Account.MIN_BALANCE || balance>Account.MAX_BALANCE)
			return;
		this.balance = balance;		
	}
	

}
