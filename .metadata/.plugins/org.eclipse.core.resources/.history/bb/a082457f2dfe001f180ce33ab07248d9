package ch01.ex06;

public class Account {
	
	private int balance;

	static final int MIN_BALANCE = 0;
	static final int MAX_BALANCE = 1000000;
	
	int getBalance() {
		return balance;
	}
	
	void setBalance(int balance) {
		if(balance >= MIN_BALANCE || balance <= MAX_BALANCE)
			return;
		this.balance = balance;		
	}
	

}
