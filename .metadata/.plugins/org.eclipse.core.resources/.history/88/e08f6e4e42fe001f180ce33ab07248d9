package ch01.ex08;

public class A {
	private String name;
	private int age;
	
	void funcA() {
		System.out.println("funcA()");
	}
	
	void info() {
		System.out.println(name);
		System.out.println(age);
	}

}

class B extends A{
	private String address;
	
	void funcB() {
		System.out.println("funcB()");
	}
	
}
