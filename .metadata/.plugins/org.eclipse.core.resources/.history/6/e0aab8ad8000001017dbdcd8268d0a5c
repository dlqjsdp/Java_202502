package ex03;

public class Human {
	String name;
	int age;
	int height;
	int weight;
	
	Human(){}
	
	Human(String name, int age, int height, int weight){
		this.name = name;
		this.age = age;
		this.height = height;
		this.weight = weight;
	}
	
	String getPrintInformation() {
		System.out.println(name + " " + age + " " + height + " " + weight + " ");
		return name + " " + age + " " + height + " " + weight + " ";
	}
	

}
