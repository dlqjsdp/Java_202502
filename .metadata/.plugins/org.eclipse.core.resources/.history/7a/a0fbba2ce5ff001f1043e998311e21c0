package ex06;

public class Book {
	
	private String title;
	private String author;
	private int price;
	String BookInfo;
	
	Book(){}
	
	Book(String title, String author, int price){
		this.title = title;
		this.author = author;
		this.price = price;
		
	}
	
	String getBookInfo() {
		return "title:" + title + ", author:" + author 
				+ ", price:" + price;
	}


}
