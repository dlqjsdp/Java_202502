package ch01.ex02;

public class CarMain {

	public static void main(String[] args) {
		
		Car car1 = new Car();
		//public Car() {} 실행
		car1.displayInfo();
		car1.runState();
		System.out.println();
		
		Car car2 = new Car("기아", "K5", "흰색", 200);
		//public Car(String company, String model, String color, int maxSpeed) 실행
		car2.displayInfo();
		System.out.println();

		
	}

}
