package ex04;


public class FriendTest {

	public static void main(String[] args) {
		
		Friend friend [] = new Friend[5];
		
		friend[0] = new Friend("김땡땡", "010-1111-1111", "aaaa@aaaaa.com");
		friend[1] = new Friend("이땡땡", "010-2222-2222", "bbbb@bbbbb.com");
		friend[2] = new Friend("박땡땡", "010-3333-1111", "cccc@ccccc.com");
		friend[3] = new Friend("최땡땡", "010-4444-1111", "dddd@ddddd.com");
		friend[4] = new Friend("김땡땡", "010-5555-1111", "eeee@eeeee.com");
		
		for(int i=0; i<friend.length; i++) {
			System.out.println(friend[i].getInfo());
		}

	}

}
