package ch04.ex01;

public class 연습 {

	public static void main(String[] args) {

		int[] num = new int[10];
		
		for(int i=0; i<num.length; i++) {
			num[i] = (int)(Math.random()*100)+1; //10개의 방에 무작위루 1~!00까지의 숫자가 들어감
		}
		
		//배열값 출력
		for(int i=0; i<num.length; i++)
			System.out.println(num[i] + " ");
	}

}
